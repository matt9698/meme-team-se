<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.layout.Background?>
<?import property_tycoon.view.PropertyPosition?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.control.Label?>

<fx:root fx:id="root" type="PropertyPosition" xmlns:fx="http://javafx.com/fxml">
    <top>
        <Region fx:id="banner" background="${root.bannerBackground}" />
    </top>
    <center>
        <VBox alignment="CENTER">
            <Label fx:id="text" text="${root.text}" />
        </VBox>
    </center>
    <stylesheets>
        <URL value="@PropertyPosition.css" />
    </stylesheets>
</fx:root>
